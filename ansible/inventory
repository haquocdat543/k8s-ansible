[all:children]
cluster
loadbalancer

[cluster:children]
masters
workers

[loadbalancer]
loadbalancer ansible_host= server_name=loadbalancer

[firstmaster]
master1-instance ansible_host= server_name=master1-instance

[othermasters]
master2-instance ansible_host= server_name=master2-instance

[masters:children]
firstmaster
othermasters

[workers]
worker1-instance ansible_host= server_name=worker1-node
worker2-instance ansible_host= server_name=worker2-node

[masters:vars]
ansible_ssh_user=gcp
ansible_ssh_private_key_file=~/.ssh/Window2

[workers:vars]
ansible_ssh_user=gcp
ansible_ssh_private_key_file=~/.ssh/Window2

